import uuid
import datetime

'''
Created on 04.05.2015

@author: Patrick Spoeth
'''


class cParticipant(object):
    '''
    classdocs
    '''


    def __init__(self, firstName="", lastName=""):
        '''
        Constructor
        '''
        self.uuid = uuid.uuid4()
        self.firstName = firstName
        self.lastName = lastName
        self.birthday = datetime.date()