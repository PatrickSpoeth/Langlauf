import datetime
import uuid

'''
Created on 04.05.2015

@author: Patrick Spoeth
'''


class cParticipant(object):
    '''
    classdocs
    '''


    def __init__(self, sFirstName="", sLastName="", sTeam=""):
        '''
        Constructor
        '''
        # eindeutige ID
        
        #self.uuid = uuid.uuid4()
        self.uuid = ""
        self.firstName = sFirstName
        self.lastName = sLastName
        self.team = sTeam
        
        self.birthday = datetime.date()

    def getTeam(self):
        return self.__team


    def setTeam(self, value):
        self.__team = value

    _team = property(getTeam, setTeam, None, None)
    
    
        
    